-- 테이블 db_even_last_3.t_user_agreements 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_user_agreements` (
  `iclause` int(10) unsigned NOT NULL,
  `iuser` int(10) unsigned NOT NULL,
  `agreement` char(1) NOT NULL CHECK (`agreement` in ('Y','N')),
  `created_at` datetime NOT NULL,
  PRIMARY KEY (`iclause`,`iuser`),
  KEY `iuser` (`iuser`),
  CONSTRAINT `t_user_agreements_ibfk_1` FOREIGN KEY (`iclause`) REFERENCES `t_signup_clause` (`iclause`),
  CONSTRAINT `t_user_agreements_ibfk_2` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- 테이블 데이터 db_even_last_3.t_user_agreements:~0 rows (대략적) 내보내기

-- 테이블 db_even_last_3.t_user_child 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_user_child` (
  `ichild` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iuser` int(10) unsigned NOT NULL,
  `gender` char(1) NOT NULL CHECK (`gender` in ('M','W')),
  `ichild_age` int(10) unsigned NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`ichild`),
  KEY `iuser` (`iuser`),
  KEY `ichild_age` (`ichild_age`),
  CONSTRAINT `t_user_child_ibfk_1` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`),
  CONSTRAINT `t_user_child_ibfk_2` FOREIGN KEY (`ichild_age`) REFERENCES `t_child_age` (`ichild_age`)
) ENGINE=InnoDB AUTO_INCREMENT=35 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

