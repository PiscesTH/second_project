-- 테이블 db_even_last_3.t_review 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_review` (
  `ireview` int(10) unsigned NOT NULL,
  `iorder` int(10) unsigned NOT NULL,
  `iproduct` int(10) unsigned NOT NULL,
  `iuser` int(10) unsigned NOT NULL,
  `req_review_pic` varchar(1000) DEFAULT NULL,
  `contents` varchar(300) DEFAULT NULL,
  `product_score` tinyint(5) unsigned NOT NULL DEFAULT 1,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`ireview`),
  UNIQUE KEY `iorder_iproduct_iuser` (`iorder`,`iproduct`,`iuser`),
  KEY `iorder` (`iorder`),
  KEY `iproduct` (`iproduct`),
  KEY `FK_t_review_t_order_2` (`iuser`),
  CONSTRAINT `FK_t_review_t_order` FOREIGN KEY (`iorder`) REFERENCES `t_order` (`iorder`),
  CONSTRAINT `FK_t_review_t_order_2` FOREIGN KEY (`iuser`) REFERENCES `t_order` (`iuser`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `t_review_ibfk_1` FOREIGN KEY (`ireview`) REFERENCES `t_order_details` (`idetails`),
  CONSTRAINT `t_review_ibfk_3` FOREIGN KEY (`iproduct`) REFERENCES `t_order_details` (`iproduct`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

