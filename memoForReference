-- 테이블 db_even_last_3.t_order_details 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_order_details` (
  `idetails` int(10) unsigned NOT NULL,
  `iorder` int(10) unsigned NOT NULL,
  `iproduct` int(10) unsigned NOT NULL,
  `product_cnt` int(10) unsigned NOT NULL,
  `product_price` int(10) unsigned NOT NULL,
  `product_total_price` int(10) unsigned NOT NULL,
  `refund_fl` tinyint(4) NOT NULL DEFAULT 0,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`iorder`,`iproduct`),
  UNIQUE KEY `idetails` (`idetails`),
  KEY `iproduct` (`iproduct`),
  CONSTRAINT `FK_t_order_details_t_order` FOREIGN KEY (`iorder`) REFERENCES `t_order` (`iorder`),
  CONSTRAINT `t_order_details_ibfk_2` FOREIGN KEY (`iproduct`) REFERENCES `t_product` (`iproduct`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

