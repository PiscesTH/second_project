-- 테이블 db_even_last_3.t_board_comment 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_board_comment` (
  `icomment` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iboard` int(10) unsigned NOT NULL,
  `iuser` int(10) unsigned NOT NULL,
  `comment` varchar(1000) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`icomment`),
  KEY `iboard` (`iboard`),
  KEY `iuser` (`iuser`),
  CONSTRAINT `t_board_comment_ibfk_1` FOREIGN KEY (`iboard`) REFERENCES `t_board` (`iboard`),
  CONSTRAINT `t_board_comment_ibfk_2` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- 테이블 데이터 db_even_last_3.t_board_comment:~1 rows (대략적) 내보내기
INSERT INTO `t_board_comment` (`icomment`, `iboard`, `iuser`, `comment`, `created_at`, `updated_at`) VALUES
	(2, 1, 19, '댓글을 써보아요 후햐햐', '2024-01-24 13:00:05', NULL);

-- 테이블 db_even_last_3.t_board_pics 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_board_pics` (
  `iboard_pic` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iboard` int(10) unsigned NOT NULL,
  `board_pic` varchar(2500) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`iboard_pic`),
  KEY `iboard` (`iboard`),
  CONSTRAINT `FK_t_board_pics_t_board` FOREIGN KEY (`iboard`) REFERENCES `t_board` (`iboard`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- 테이블 데이터 db_even_last_3.t_board_pics:~0 rows (대략적) 내보내기

-- 테이블 db_even_last_3.t_child_age 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_child_age` (
  `ichild_age` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `child_age` varchar(20) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`ichild_age`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

