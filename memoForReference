-- 테이블 db_even_last_3.t_middle_category 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_middle_category` (
  `imain` int(10) unsigned NOT NULL,
  `imiddle` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `middle_category` varchar(20) NOT NULL,
  `created_at` date NOT NULL DEFAULT current_timestamp(),
