CREATE TABLE IF NOT EXISTS `t_refund` (
  `irefund` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `idetails` int(10) unsigned NOT NULL,
  `contents` varchar(200) NOT NULL,
  `refund_cnt` int(11) NOT NULL,
  `refund_price` int(11) NOT NULL,
  `complete_fl` tinyint(4) NOT NULL DEFAULT 0,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`irefund`),
  KEY `iproduct` (`idetails`),
  CONSTRAINT `t_refund_ibfk_1` FOREIGN KEY (`idetails`) REFERENCES `t_order_details` (`idetails`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

