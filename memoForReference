-- 테이블 db_even_last_3.t_product 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_product` (
  `iproduct` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `imain` int(10) unsigned NOT NULL,
  `imiddle` int(10) unsigned NOT NULL,
  `product_nm` varchar(150) NOT NULL,
  `product_details` varchar(1000) DEFAULT NULL,
  `recommand_age` tinyint(4) NOT NULL DEFAULT 0,
  `price` int(10) unsigned NOT NULL,
  `rep_pic` varchar(1000) NOT NULL,
  `remained_product` int(10) unsigned NOT NULL,
  `new_fl` int(10) unsigned NOT NULL DEFAULT 0,
  `rc_fl` int(10) unsigned NOT NULL DEFAULT 0,
  `pop_fl` int(10) unsigned NOT NULL DEFAULT 0,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`iproduct`),
  KEY `t_product_t_middle_category` (`imain`,`imiddle`) USING BTREE,
  KEY `FK_t_product_t_middle_category_2` (`imiddle`),
  CONSTRAINT `FK_t_product_t_middle_category` FOREIGN KEY (`imain`) REFERENCES `t_middle_category` (`imain`),
  CONSTRAINT `FK_t_product_t_middle_category_2` FOREIGN KEY (`imiddle`) REFERENCES `t_middle_category` (`imiddle`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=31 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

