-- 테이블 db_even_last_3.t_basket 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_basket` (
  `iuser` int(10) unsigned NOT NULL,
  `iproduct` int(10) unsigned NOT NULL,
  `product_cnt` int(10) unsigned NOT NULL,
  `price` int(10) unsigned NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`iuser`,`iproduct`),
  KEY `iproduct` (`iproduct`),
  KEY `iuser` (`iuser`),
  CONSTRAINT `FK_t_basket_t_product` FOREIGN KEY (`iproduct`) REFERENCES `t_product` (`iproduct`),
  CONSTRAINT `t_basket_ibfk_1` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- 테이블 데이터 db_even_last_3.t_basket:~0 rows (대략적) 내보내기

-- 테이블 db_even_last_3.t_board 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_board` (
  `iboard` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iuser` int(10) unsigned NOT NULL,
  `board_code` tinyint(4) NOT NULL,
  `title` varchar(100) NOT NULL,
  `contents` varchar(2500) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`iboard`),
  KEY `iuser` (`iuser`),
  CONSTRAINT `t_board_ibfk_1` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`)
) ENGINE=InnoDB AUTO_INCREMENT=276 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

