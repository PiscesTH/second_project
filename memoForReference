-- 테이블 db_even_last_3.t_order 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_order` (
  `iorder` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iuser` int(10) unsigned NOT NULL,
  `iaddress` int(10) unsigned NOT NULL,
  `ipayment_option` int(10) unsigned NOT NULL DEFAULT 1,
  `addressee_nm` varchar(10) DEFAULT NULL,
  `phone_number` varchar(13) DEFAULT NULL,
  `email` varchar(50) DEFAULT NULL,
  `process_state` tinyint(4) unsigned NOT NULL DEFAULT 0,
  `total_price` int(10) unsigned NOT NULL,
  `delivery_request` varchar(100) DEFAULT NULL,
  `delete_fl` int(11) unsigned NOT NULL DEFAULT 0,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  `updated_at` datetime DEFAULT NULL ON UPDATE current_timestamp(),
  PRIMARY KEY (`iorder`),
  KEY `iuser` (`iuser`),
  KEY `iaddress` (`iaddress`),
  KEY `FK_t_order_t_payment_option` (`ipayment_option`),
  CONSTRAINT `FK_t_order_t_payment_option` FOREIGN KEY (`ipayment_option`) REFERENCES `t_payment_option` (`ipayment_option`),
  CONSTRAINT `t_order_ibfk_1` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`),
  CONSTRAINT `t_order_ibfk_2` FOREIGN KEY (`iaddress`) REFERENCES `t_address` (`iaddress`)
) ENGINE=InnoDB AUTO_INCREMENT=100007 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

