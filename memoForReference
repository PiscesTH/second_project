-- 테이블 db_even_last_3.t_wish_list 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_wish_list` (
  `iuser` int(10) unsigned NOT NULL,
  `iproduct` int(10) unsigned NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`iuser`,`iproduct`),
  KEY `iproduct` (`iproduct`),
  KEY `iuser` (`iuser`),
  CONSTRAINT `t_wish_list_ibfk_1` FOREIGN KEY (`iuser`) REFERENCES `t_user` (`iuser`),
  CONSTRAINT `t_wish_list_ibfk_2` FOREIGN KEY (`iproduct`) REFERENCES `t_product` (`iproduct`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

