-- 테이블 db_even_last_3.t_product_pics 구조 내보내기
CREATE TABLE IF NOT EXISTS `t_product_pics` (
  `iproduct_pics` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `iproduct` int(10) unsigned NOT NULL,
  `product_pic` varchar(1000) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`iproduct_pics`),
  KEY `iproduct` (`iproduct`),
  CONSTRAINT `t_product_pics_ibfk_1` FOREIGN KEY (`iproduct`) REFERENCES `t_product` (`iproduct`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

